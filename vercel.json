{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js",
      "scripts": {
        "build": "tsc && cpy src/config/serviceAccountKey.json dist/config",
        "start": "node dist/index.js"
      }
    }
  ]
}
